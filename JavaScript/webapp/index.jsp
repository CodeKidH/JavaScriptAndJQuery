<html>
<body>
<h2>Method of Array</h2>

<script type="text/javascript">

	/*
		join(separator)
		
		- result = apple/grape/banana
	*/
	
	var fruits = ['apple','grape','banana'];
	var str = fruits.join('/');
	document.write(str);
	
	/* 
		reverse() 
		
		- result = banaan,grape,apple
	*/
	
	var fruits = ['apple','grape','banaan'];
	var arr = fruits.reverse();
	document.write(arr);
	
	
	
	
</script>
</body>
</html>
